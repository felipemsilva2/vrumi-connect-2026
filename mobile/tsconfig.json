{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/integrations/*": [
        "../src/integrations/*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@contexts/*": [
        "./contexts/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}